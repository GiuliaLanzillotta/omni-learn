<?xml version="1.0" ?>
<mujoco model="MuJoCo Model">
    <compiler angle="radian"/>
    <statistic extent="1" />
    <option gravity="0 0 0" integrator="RK4" timestep='0.02'/>
    <size njmax="795" nconmax="250" nstack="631875" />
    <visual>
        <quality shadowsize="2048" />
        <!--<map znear="0.8" zfar="3.2"/>-->
        <!--<map znear=".1" zfar="10"/>-->
        <!--<map znear=".9" zfar="5.25"/>-->
        <headlight active="0"/>
        <map znear=".5" zfar="5.2"/>
    </visual>
    <default class="main">
        <joint limited="true" armature="0.01" damping="0.7" /> <!-- damping was 0.2-->
        <!-- <position kp="500" /> -->
        <geom margin="0.001" />
    </default>
    <asset>
        <!-- Fill in textures and materials -->

        <!-- eg -->
        <!--<texture builtin="flat" name="texture1" height="32" width="32" rgb1="1 1 1" type="cube"/>-->
        <!--<material name="material1" texture="texture1" />-->

        {}

        <!--<texture builtin="flat" name="floor_tex" height="32" width="32" rgb1=".6 .6 .6" type="cube"/>-->
        <material name="screen_mat" emission="10" shininess="0" specular="0" reflectance="0" rgba="0 0 0 0"/>
    </asset>
    <worldbody>
        <!--<geom name="floor0" size="1 1 1" pos="0 0 0" type="plane" material="floor_mat" />-->

        <!--<light pos="1 0 1.5" dir="-0.371391 0 -0.928477" castshadow="false" cutoff="90" exponent="1" diffuse="1 1 1" specular="0 0 0" />-->

        <!--<geom name="obj_box" size=".5 .5 .5" pos="0 0 0" type="box" rgba="1 0 0 0.2"/>-->

        <!--<body name="screen" pos="-.7 0 0">-->
            <!--<geom name="screen" type="box" size=".01 2 2" material="screen_mat" />-->
        <!--</body>-->

        <body name="camera" euler="0 1.57 1.57" pos="2.2 0 0">
            <light directional="true" pos="0 0 0"/>
            <camera fovy="45" name="external_camera_0" pos="0 0 0" />
            <!--<geom name="cm1" size=".1 .1 .1" pos="0 0 0" type="box" rgba="0 0 1 0.5"/>-->
            <!--<geom name="cm2" size=".05 .05 .05" pos="0 0 -.1" type="box" rgba="0 0 1 0.5"/>-->
            <!--<geom name="cm3" size="1 .5 .1" pos="0 0.5 -1" type="box" rgba="0 1 0 0.5"/>-->
            <!--<geom name="cm4" size="1 1 .1" pos="0 0 -3" type="box" rgba="0 1 0 1"/>-->
            <!--<geom name="cm2" size=".05 .05 .05" pos="0 .1 0" type="box" rgba="0 0 1 0.5"/>-->
            <!--<geom name="cm2" size=".05 .05 .05" pos="0 0 .1" type="box" rgba="0 0 1 0.5"/>-->
        </body>

        <body name="floating_obj" pos="0 0 0">

            <inertial pos="0 0 0" mass="1" diaginertia=".25 .25 .25"/>


            <joint name="obj_jnt_x" pos="0 0 0" axis="1 0 0" type="slide" limited='true' range="-.5 .5" damping="0.01" />
            <joint name="obj_jnt_y" pos="0 0 0" axis="0 1 0" type="slide" limited='true' range="-.5 .5" damping="0.01" />
            <joint name="obj_jnt_z" pos="0 0 0" axis="0 0 1" type="slide" limited='true' range="-.5 .5" damping="0.01" />

            <joint name="obj_jnt_wx" pos="0 0 0" axis="1 0 0" type="hinge" limited='false' damping="0.01" />
            <joint name="obj_jnt_wy" pos="0 0 0" axis="0 1 0" type="hinge" limited='false' damping="0.01" />
            <joint name="obj_jnt_wz" pos="0 0 0" axis="0 0 1" type="hinge" limited='false' damping="0.01" />

            {}
        </body>


    </worldbody>
    <actuator>

        <motor gear="16" joint="obj_jnt_x" name="obj:motionX"/>
        <motor gear="16" joint="obj_jnt_y" name="obj:motionY"/>
        <motor gear="16" joint="obj_jnt_z" name="obj:motionZ"/>
        <motor gear="4" joint="obj_jnt_wx" name="obj:motionWX"/>
        <motor gear="4" joint="obj_jnt_wy" name="obj:motionWY"/>
        <motor gear="4" joint="obj_jnt_wz" name="obj:motionWZ"/>

    </actuator>
</mujoco>
