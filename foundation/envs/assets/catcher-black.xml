<!-- Author: vikash@openai.com-->
<mujoco model='tosser-v1.5'>
    <compiler inertiafromgeom='auto' angle='radian'/>

    <default>
        <joint limited='true' damping='1' armature='0'/>
        <geom contype='1' conaffinity='1' condim='3' rgba='0.8 0.6 .4 1'
        	margin="0.001" solref=".02 1" solimp=".8 .8 .01" material="geom"/>
        <motor ctrlrange='-1 1'  ctrllimited='true'/>
    </default>

    <option timestep='0.002' iterations="50" solver="PGS">
        <flag energy="enable"/>
    </option>

    <visual>
        <map fogstart="3" fogend="5" force="0.1" znear="0.1" zfar="10"/>
        <quality shadowsize="2048"/>
        <global offwidth="800" offheight="800"/>
    </visual>
	<statistic extent="1" />

    <asset>
		<!--<texture name="lbl_tex" builtin="flat" width="32" height="32" rgb1="0 0 0"/>-->
		<!--<texture type="skybox" width="128" height="128" rgb1="0 0 0" rgb2="0 0 0"/>  -->
        <texture name="texgeom" type="cube" builtin="flat" mark="cross" width="127" height="1278" 
            rgb1="0.8 0.6 0.4" rgb2="0.8 0.6 0.4" markrgb="1 1 1" random="0.01"/>
		<!--<texture name="texplane" type="cube" rgb1="0 0 0" rgb2="0 0 0"-->
            <!--width="512" height="512"/>-->
        <!--<texture name="texplane" type="2d" builtin="checker" rgb1=".2 .3 .4" rgb2=".1 0.15 0.2" -->
            <!--width="512" height="512"/>-->
		<!--<texture name="texobj" type="2d" builtin="checker" rgb1=".9 .5 .1" rgb2=".3 .9 .1"-->
            <!--width="512" height="512"/>-->
		<texture name="texobj" type="2d" builtin="checker" rgb1="1 1 0" rgb2="1 1 0"
            width="512" height="512"/>


        <!--<material name='MatPlane' reflectance='0.5' texture="texplane" texrepeat="1 1" texuniform="true"/>-->
		<material name='lbl_mat' texuniform="true" emission="1000" specular="0" shininess="0" reflectance="0"/>
		<!--<material name='MatPlane' reflectance='0' texture="texplane" texrepeat="1 1" texuniform="true"/>-->
		<material name='MatObj' reflectance='1' specular="1" emission="0.5" shininess="1" texture="texobj" texrepeat="1 1" texuniform="true"/>
        <material name='geom' texuniform="true"/>
	</asset>

    <worldbody>     
        <light directional='false' diffuse='.8 .8 .8' specular='0.3 0.3 0.3' pos='0 0 4.0' dir='0 -.15 -1'/>
        <light directional='false' diffuse='.8 .8 .8' specular='0.3 0.3 0.3' pos='0 -2 4.0' dir='0 .15 -1'/>

		<geom name='floor' pos='0 -1 -0.03' size='.1 3 .125' type='plane' rgba="0 0 0 1" condim='3'/>
		<!--<site name='lost' pos='0 -1 0' type='box' group='4' size='0.1 0.9 .01' rgba="0 1 0 1"/>-->
		<!--<site name='lost' pos='0 -1 0' type='box' group='4' size='0.1 1 .01' rgba="0 1 0 1"/>-->
		<!--<geom name='lost2' pos='0 -1 0' type='box' group='4' size='0.1 1 .01' rgba="0 1 0 1"/>-->

		<body name="cam" pos="1.7 -1 0.65" euler="1.57 1.57 0">
			<camera name="cam" mode="fixed" fovy="45"/>
		</body>


		<body name='basket0' pos='0 -1 0'>
			<geom name="b0" pos='0 -.1 .12' type='capsule' size='.02 .1' rgba='0.91 0.85 0.76 1' condim='1' mass=".6" euler='.2 0 0'/>
			<geom name="b1" pos='0  .1 .12' type='capsule' size='.02 .1' rgba='0.91 0.85 0.76 1' condim='1' mass=".6" euler='-.2 0 0'/>
			<geom name="b2" pos='0  0 .02' type='capsule' size='.02 .08' rgba='0.91 0.85 0.76 1' condim='1' mass=".8" euler='1.57 0 0'/>
			<site name='t0' pos='0 0 .17' type='box' group='4' size='.05 .05 .1' rgba="0 1 0 0.3"/>
			<!--<geom name='t0' pos='0 0 .16' type='box'  size='.06 .06 .11' rgba="0 1 0 1"/>-->
			<joint name='movey' type='slide' axis='0 1 0' limited='true' range="-.75 .75" armature="0"/>
		</body>

		<body name='obj' pos='0 -.08 1'>
			<!--<geom name='obj' type='box' size='.05 .05 0.05'/>-->
			<!--<geom name='obj' type='sphere' size='.05'/>-->

			<geom name='gobj' type='capsule' size='.05 .07' pos="0 0 0.0" mass="1.8" material="MatObj" />
			<!--<geom name='obj' type='capsule' size='.05 .035' pos="0 0 0.04" rgba="1 .3 .1 1" mass=".7" />-->
			<!--<geom name='obj2' type='capsule' size='.0499 .035' pos="0 0 -0.04" rgba=".1 .4 1 1" mass=".7"/>-->

			<joint name='bally' type='slide' axis='0 1 0' limited='false' damping='.01'/>
			<joint name='ballz' type='slide' axis='0 0 1' limited="false" damping='.01'/>
			<joint name='ballx' type='hinge' axis='1 0 0' limited='false' damping='.01'/>
		</body>

		<body name='wall' pos='0 0 0'>
			<geom name='wall' pos='0 0 .5' size='.02 .6' type='capsule' rgba='.73 .4 .4 1'/>
			<body name='hand' pos='0 0 0'>
				<geom name='palm' type='capsule' size='.02' fromto='0 0 .7 0 -.1 .6'/>
				<geom name='finger' type='capsule' size='.02' fromto='0 -.1 .6 0 -.15 .6'/>
				<joint name='wr_js' type='slide' pos='0 0 .7' axis='0 0 1' range='-.6 .25' damping='10'/>
				<joint name='wr_jr' type='hinge' pos='0 0 .7' axis='1 0 0' range='-1.57 0.8'/>
			</body>
		</body>
	</worldbody>

    <actuator>
		<motor name='catcher_y' gear='20' joint='movey'/>
		<motor name='a1' gear='35' joint='wr_js'/>
		<motor name='a2' gear='35' joint='wr_jr'/>
    </actuator>
	
	<sensor>
		<jointpos name="S_move_pos"	joint="movey"/>
		<jointvel name="S_move_vel"	joint="movey"/>


		<jointpos name="pos_by"	joint="bally"/>
		<jointpos name="pos_bz"	joint="ballz"/>
		<jointpos name="pos_bx"	joint="ballx"/>

		<jointvel name="vel_by"	joint="bally"/>
		<jointvel name="vel_bz"	joint="ballz"/>
		<jointvel name="vel_bx"	joint="ballx"/>

		<jointpos name="wr_x"	joint="wr_js"/>
		<jointpos name="wr_t"	joint="wr_jr"/>

		<jointvel name="wr_v"	joint="wr_js"/>
		<jointvel name="wr_w"	joint="wr_jr"/>

		<!--<jointpos name="Sjp_wr_js"	joint="wr_js"/>-->
		<!--<jointpos name="Sjp_wr_jr"	joint="wr_jr"/>-->

		<touch name="s_t1"	site="t0"/>
		<!--<touch name="hit_floor" site="lost"/>-->
	</sensor>

</mujoco>
